<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Get-Assertion</Name>
            </Step>
            <Step>
                <Name>Get-Private-Key</Name>
            </Step>
            <Step>
                <Name>SAML-Test</Name>
            </Step>
            <Step>
                <Name>Validate-SAML-Assertion-1</Name>
            </Step>
            <Step>
                <Name>Assign-Message-1</Name>
            </Step>
            <Step>
                <Name>OAuth-v20-1</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPProxyConnection>
        <BasePath>/samltest</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="noroute"/>
</ProxyEndpoint>